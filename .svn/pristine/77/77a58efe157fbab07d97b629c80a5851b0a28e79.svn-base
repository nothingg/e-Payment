CREATE OR REPLACE PROCEDURE B_P_GAM (  
    FORACID_IN IN VARCHAR2,
    OUT_CURSOR    OUT SYS_REFCURSOR)
IS
BEGIN
    OPEN OUT_CURSOR FOR
         SELECT 
            acid , sol_id , foracid , acct_name , schm_code , clr_bal_amt
          FROM tbaadm.gam where foracid = FORACID_IN;
    END B_P_GAM;

    