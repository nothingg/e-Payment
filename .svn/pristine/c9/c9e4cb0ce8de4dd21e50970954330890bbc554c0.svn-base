	<input id="ITMXFirstName" name="ITMXFirstName" type="text" placeholder="ข้อมูลเป็นภาษาอังกฤษ" class="form-control input-md" data-validation="custom length" data-validation-regexp="([a-zA-Z]+)" data-validation-length="max5" data-validation-error-msg-custom ="Plz English Only" data-validation-error-msg-length="Max 32 Char" >
-->

	<script type="text/javascript" src="js/jquery-1.9.1.js" ></script>
	<script type="text/javascript" src="datepicker/bootstrap-datepicker.js"></script>
	<script type="text/javascript" src="script-anyid.js"></script>

	<script type="text/javascript" src="form-validator/jquery.form-validator.min.js"></script>
	<script type="text/javascript">
		$.validate();
	</script>
</body>
</html>